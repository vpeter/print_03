<h1>Editing paper_type</h1>

<% form_for(@paper_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :paper_category_id %><br />
    <%= f.text_field :paper_category_id %>
  </p>
  <p>
    <%= f.label :nev %><br />
    <%= f.text_field :nev %>
  </p>
  <p>
    <%= f.label :leiras %><br />
    <%= f.text_area :leiras %>
  </p>
  <p>
    <%= f.label :kep %><br />
    <%= f.text_field :kep %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @paper_type %> |
<%= link_to 'Back', paper_types_path %>