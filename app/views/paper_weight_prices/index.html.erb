<h1>Listing paper_weight_prices</h1>

<table>
  <tr>
    <th>Paper type</th>
    <th>Gramm</th>
    <th>Ar</th>
    <th>Kep</th>
  </tr>

<% @paper_weight_prices.each do |paper_weight_price| %>
  <tr>
    <td><%=h paper_weight_price.paper_type_id %></td>
    <td><%=h paper_weight_price.gramm %></td>
    <td><%=h paper_weight_price.ar %></td>
    <td><%= link_to 'Show', paper_weight_price %></td>
    <td><%= link_to 'Edit', edit_paper_weight_price_path(paper_weight_price) %></td>
    <td><%= link_to 'Destroy', paper_weight_price, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New paper_weight_price', new_paper_weight_price_path %>